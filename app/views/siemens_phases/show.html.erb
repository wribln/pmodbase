<%- page_title %>
<div class="container">
  <%= breadcrumbs %>
  <%= form_title %>
  <div class="form-horizontal" role="form">
    <%= form_for @siemens_phase do |sp| %>
      <%= render partial: 'shared/notice' %>
      <div class="form-group">
        <%= sp.label :id, class: 'col-sm-3 control-label' %>
        <div class="col-sm-2">
          <%= display_value sp.object.to_id %>
        </div>
      </div>
      <div class="form-group">
        <%= sp.label :code, class: 'col-sm-3 control-label' %>
        <div class="col-sm-2">
          <%= display_value sp.object.code %>
        </div>
      </div>
      <div class="form-group">
        <%= sp.label :label_p, class: 'col-sm-3 control-label' %>
        <div class="col-sm-4">
          <%= display_value sp.object.label_p %>
        </div>
      </div>
      <div class="form-group">
        <%= sp.label :label_m, class: 'col-sm-3 control-label' %>
        <div class="col-sm-4">
          <%= display_value sp.object.label_m %>
        </div>
      </div>
      <div class="form-group">
        <div class="col-sm-offset-3 col-sm-2">
          <%= link_to t( 'button_label.edit' ), edit_siemens_phase_path( @siemens_phase ), class: 'btn btn-lg btn-block btn-default' %>
        </div>
        <div class="col-sm-2">
          <%= link_to t( 'button_label.go_to_index' ), siemens_phases_path, class: 'btn btn-lg btn-block btn-default' %>
        </div>
      </div>
    <% end %>
  </div>
</div>
