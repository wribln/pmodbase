<%- page_title %>
<div class="container">
  <%= breadcrumbs %>
  <%= form_title %>
  <div class="table-responsive">
    <table class="table table-striped table-bordered">
      <thead>
        <tr>
          <th><%= SiemensPhase.human_attribute_name( :id      ) %></th>
          <th><%= SiemensPhase.human_attribute_name( :code    ) %></th>
          <th><%= SiemensPhase.human_attribute_name( :label_p ) %></th>
          <th><%= SiemensPhase.human_attribute_name( :label_m ) %></th>
          <th><%= t( 'action_title.header' ) %></th>
        </tr>
      </thead>
      <tbody>
        <% if @siemens_phases.length.zero? %>
          <%= render partial: 'shared/index_empty_row', locals:{ columns: 4, with_link: true }%>
        <% else %>
          <% @siemens_phases.each do |sp| %>
            <tr>
              <td><%= sp.to_id %></td>
              <td><%= sp.code %></td>
              <td><%= sp.label_p %></td>
              <td><%= sp.label_m %></td>
              <td><%= render partial: 'shared/actions', object: sp %></td>
            </tr>
          <% end %>
        <% end %>
      </tbody>
    </table>
    <%= render partial: 'shared/footer' %>
  </div>
</div>


