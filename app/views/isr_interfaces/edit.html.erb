<%- page_title %>
<div class="container">
  <%= breadcrumbs %>
  <%= form_title %>
  <div class="form-horizontal" role="form">
    <%= form_for @isr_interface do |f| %>
      <%= render partial: 'shared/errors', object: @isr_interface.errors %>
      <%= render partial: 'shared/notice' %>
      <%- if @isr_interface.frozen? -%>
        <%= render partial: 'show_if_all', object: f, locals:{ note: :edit }%>
      <%- else -%>
        <%= render partial: 'form_if', object: f, locals:{ note: :edit } %>
      <%- end -%>
      <div class="form-group">
        <div class="col-sm-offset-3 col-sm-2">
          <%= f.submit class: 'btn btn-lg btn-block btn-primary' %>
        </div>
        <div class="col-sm-2">
          <%= link_to t( 'button_label.cancel_to_show' ), isr_interface_details_path( @isr_interface ), class: 'btn btn-lg btn-block btn-default' %>
        </div>
      <div>
    <%- end %>
  </div>
</div>
