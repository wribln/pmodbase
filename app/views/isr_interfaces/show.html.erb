<%- page_title %>
<div class="container">
  <%= breadcrumbs %>
  <%= form_title %>
  <div class="form-horizontal" role="form">
    <%= form_for @ifr_interface do |f| %>
      <%= render partial: 'shared/notice' %>
      <div class="form-group">
        <%= f.label :id, class: 'col-sm-3 control-label' %>
        <div class="col-sm-2">
          <%= display_value f.object.to_id %>
        </div>
      </div>
      <div class="form-group">
        <%= f.label :interface_id, class: 'col-sm-3 control-label' %>
        <div class="col-sm-4">
          <%= display_value f.object.interface_id %>
        </div>
      </div>
      <div class="form-group">
        <%= f.label :leader_group_id, class: 'col-sm-3 control-label' %>
        <div class="col-sm-4">
          <%= display_value f.object.leader_group_id %>
        </div>
      </div>
      <div class="form-group">
        <%= f.label :partner_group_id, class: 'col-sm-3 control-label' %>
        <div class="col-sm-4">
          <%= display_value f.object.partner_group_id %>
        </div>
      </div>
      <div class="form-group">
        <%= f.label :title, class: 'col-sm-3 control-label' %>
        <div class="col-sm-4">
          <%= display_value f.object.title %>
        </div>
      </div>
      <div class="form-group">
        <%= f.label :desc, class: 'col-sm-3 control-label' %>
        <div class="col-sm-4">
          <%= display_value f.object.desc %>
        </div>
      </div>
      <div class="form-group">
        <%= f.label :type, class: 'col-sm-3 control-label' %>
        <div class="col-sm-4">
          <%= display_value f.object.type %>
        </div>
      </div>
      <div class="form-group">
        <%= f.label :current_status, class: 'col-sm-3 control-label' %>
        <div class="col-sm-4">
          <%= display_value f.object.current_status %>
        </div>
      </div>
      <div class="form-group">
        <%= f.label :current_task, class: 'col-sm-3 control-label' %>
        <div class="col-sm-4">
          <%= display_value f.object.current_task %>
        </div>
      </div>
      <div class="form-group">
        <%= f.label :cfr_record_id, class: 'col-sm-3 control-label' %>
        <div class="col-sm-4">
          <%= display_value f.object.cfr_record_id %>
        </div>
      </div>
      <div class="form-group">
        <%= f.label :safety_related, class: 'col-sm-3 control-label' %>
        <div class="col-sm-4">
          <%= display_value f.object.safety_related %>
        </div>
      </div>
      <div class="form-group">
        <div class="col-sm-offset-3 col-sm-2">
          <%= link_to t( 'button_label.edit' ), edit_ifr_interface_path( @ifr_interface ), class: 'btn btn-lg btn-block btn-default' %>
        </div>
        <div class="col-sm-2">
          <%= link_to t( 'button_label.go_to_index' ), ifr_interfaces_path, class: 'btn btn-lg btn-block btn-default' %>
        </div>
      </div>
    <% end %>
  </div>
</div>
