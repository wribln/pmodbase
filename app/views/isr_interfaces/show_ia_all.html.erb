<%- page_title %>
<div class="container">
  <%= breadcrumbs %>
  <%= form_title %>
  <div class="form-horizontal" role="form">
    <%= form_for @isr_interface do |f| %>
      <%= render partial: 'shared/notice' %>
      <%= render partial: 'show_if_all', object: f, locals:{ if_code: :true, note: true }%>
      <!-- Divider Line -->
      <div class="form-group">
        <hr style="margin:4px"/>  
      </div>
      <%= fields_for @isr_agreement do |f| %>
        <%= render partial: 'isr_interfaces/show_ia_all', object: f %>
      <%- end -%>
      <div class="form-group">
        <div class="col-sm-offset-3 col-sm-2">
          <%= link_to t( 'button_label.show' ), isr_agreement_path( @isr_agreement ), class: 'btn btn-lg btn-block btn-default' %>
        </div>
        <div class="col-sm-2">
          <%= link_to t( 'button_label.print' ), isr_agreement_icf_path( @isr_agreement ), class: 'btn btn-lg btn-block btn-default' %>
        </div>
        <div class="col-sm-2">
          <%= link_to t( 'button_label.go_to_index' ), isr_interfaces_path, class: 'btn btn-lg btn-block btn-default' %>
        </div>
      </div>
      <div class="form-group">
        <div class="col-sm-offset-3 col-sm-2">
          <%= link_to t( 'button_label.edit' ), edit_isr_agreement_path( @isr_agreement ), class: 'btn btn-lg btn-block btn-default' %>
        </div>
        <div class="col-sm-2">
          <%= link_to t( 'isr_interfaces.show.add_ia' ), new_isr_workflow_path( @isr_agreement ), class: 'btn btn-lg btn-block btn-default' %>
        </div>
      </div>
   <%- end -%>
  </div>
</div>
