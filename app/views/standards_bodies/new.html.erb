<%- page_title %>
<div class="container">
  <%= breadcrumbs %>
  <%= form_title %>
  <div class="form-horizontal" role="form">
    <%= form_for @standards_body do |sb| %>
      <%= render partial: 'shared/notice' %>
      <%= render partial: 'shared/errors', object: @standards_body.errors %>
      <%= render partial: 'form', object: sb %>
      <div class="form-group">
        <div class="col-sm-offset-3 col-sm-2">
          <%= sb.submit class: 'btn btn-lg btn-block btn-primary' %>
        </div>
        <div class="col-sm-2">
          <%= link_to t( 'button_label.goto_to_list' ), standards_bodies_path, class: 'btn btn-lg btn-block btn-default' %>
        </div>
      </div>
    <% end %>
  </div>
</div>