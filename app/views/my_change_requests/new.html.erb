<%- page_title %>
<div class="container">
  <%= breadcrumbs %>
  <%= form_title %>
  <div class="form-horizontal" role="form">
    <%= form_for @dbcr, url: my_change_requests_path do |d| %>
      <%= render partial: 'shared/notice' %>
      <%= render partial: 'shared/errors', object: @dbcr.errors %>
      <%= render partial: 'my_change_requests/form', object: d %>
      <div class="form-group">
        <div class="col-sm-offset-3 col-sm-2">
          <%= d.submit class: "btn btn-lg btn-block btn-primary" %>
        </div>
        <div class="col-sm-2">
          <%= link_to t( 'button_label.goto_to_list' ), my_change_requests_path, class: "btn btn-lg btn-block btn-default" %>
        </div>
        <div class="col-sm-2">
          <%= link_to_help :my_change_requests, t( 'button_label.help' ), class: "btn btn-lg btn-block btn-default" %>
        </div>
      <div>
    <% end %>
  </div>
</div>
