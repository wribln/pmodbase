<%- page_title %>
<div class="container">
  <%= breadcrumbs %>
  <%= form_title %>
  <div class="form-horizontal" role="form">
    <%= form_for @cfr_relationship_l do |f| %>
      <%= render partial: 'shared/notice' %>
      <div class="form-group">
        <%= f.label :id, class: 'col-sm-3 control-label' %>
        <div class="col-sm-2">
          <%= display_value f.object.to_id %>
        </div>
      </div>
      <div class="form-group">
        <%= f.label :rs_group, class: 'col-sm-3 control-label' %>
        <div class="col-sm-2">
          <%= display_value f.object.rs_group_label %>
        </div>
      </div>
      <div class="form-group">
        <%= f.label 'label[0]', t( '.leading_label' ), class: 'col-sm-3 control-label' %>
        <div class="col-sm-4">
          <%= display_value f.object.label %>
        </div>
      </div>
      <div class="form-group">
        <%= f.label 'label[1]', t( '.reverse_label' ), class: 'col-sm-3 control-label' %>
        <div class="col-sm-4">
          <%= display_value f.object.reverse_rs.label %>
        </div>
      </div>
      <div class="form-group">
        <div class="col-sm-offset-3 col-sm-2">
          <%= link_to t( 'button_label.edit' ), edit_cfr_relationship_path( @cfr_relationship_l ), class: 'btn btn-lg btn-block btn-default' %>
        </div>
        <div class="col-sm-2">
          <%= link_to t( 'button_label.go_to_index' ), cfr_relationships_path, class: 'btn btn-lg btn-block btn-default' %>
        </div>
      </div>
    <% end %>
  </div>
</div>
