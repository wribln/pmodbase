<%- page_title %>
<div class="container">
  <%= breadcrumbs %>
  <%= form_title %>
  <div class="form-horizontal" role="form">
    <%= form_for @pcp_category do |f| %>
      <%= render partial: 'shared/notice' %>
      <div class="form-group">
        <%= f.label :id, class: "col-sm-3 control-label" %>
        <div class="col-sm-1">
          <%= display_value f.object.id %>
        </div>
      </div>
      <div class="form-group">
        <%= f.label :label, class: "col-sm-3 control-label" %>
        <div class="col-sm-4">
          <%= display_value f.object.label %>
        </div>
      </div>
      <div class="form-group">
        <%= f.label :p_group_id, class: "col-sm-3 control-label" %>
        <div class="col-sm-2">
          <%= display_value f.object.p_group.code %>
        </div>
      </div>
      <div class="form-group">
        <%= f.label :c_group_id, class: "col-sm-3 control-label" %>
        <div class="col-sm-2">
          <%= display_value f.object.c_group.code %>
        </div>
      </div>
      <div class="form-group">
        <div class="col-sm-offset-3 col-sm-2">
          <%= link_to t( 'button_label.edit' ), edit_pcp_category_path( @pcp_category ), class: "btn btn-lg btn-block btn-default" %>
        </div>
        <div class="col-sm-2">
          <%= link_to t( 'button_label.goto_to_list' ), pcp_categories_path, class: "btn btn-lg btn-block btn-default" %>
        </div>
      </div>
    <% end %>
  </div>
</div>
