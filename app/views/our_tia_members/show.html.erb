<%- page_title %>
<div class="container">
  <%= breadcrumbs %>
  <%= form_title_w_link( @tia_list.label, my_tia_lists_path ) %>
  <div class="form-horizontal" role="form">
    <%= form_for @tia_member, url: our_tia_member_path( @tia_member ) do |f| %>
      <%= render partial: 'shared/notice' %>
      <div class="form-group">
        <%= f.label :account_id, class: "col-sm-3 control-label" %>
        <div class="col-sm-1">
          <%= f.text_field :account_id, disabled: true, readonly: true %>
        </div>
      </div>
      <div class="form-group">
        <%= f.label :to_access, class: "col-sm-3 control-label" %>
        <div class="col-sm-1">
          <%= f.check_box :to_access, disabled: true, readonly: true %>
        </div>
      </div>
      <div class="form-group">
        <%= f.label :to_update, class: "col-sm-3 control-label" %>
        <div class="col-sm-1">
          <%= f.check_box :to_update, disabled: true, readonly: true %>
        </div>
      </div>
      <div class="form-group">
        <div class="col-sm-offset-3 col-sm-2">
          <%= link_to t( 'button_label.edit' ), edit_our_tia_member_path( @tia_member ), class: "btn btn-lg btn-block btn-default" %>
        </div>
        <div class="col-sm-2">
          <%= link_to t( 'button_label.goto_to_list' ), my_tia_list_our_tia_members_path( @tia_list ), class: "btn btn-lg btn-block btn-default" %>
        </div>
      </div>
    <% end %>
  </div>
</div>
