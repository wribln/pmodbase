<%- page_title %>
<div class="container">
  <%= breadcrumbs %>
  <%= form_title %>
  <div class="form-horizontal" role="form">
    <%= form_for @tia_list, url: my_tia_list_path( @tia_list.id ) do |f| %>
      <%= render partial: 'shared/errors', object: @tia_list.errors %>
      <%= render partial: 'form', object: f %>
      <div class="form-group">
        <div class="col-sm-offset-3 col-sm-2">
          <%= f.submit class: "btn btn-lg btn-block btn-primary" %>
        </div>
        <div class="col-sm-2">
          <%= link_to t( 'button_label.show' ), my_tia_list_path( @tia_list.id ), class: "btn btn-lg btn-block btn-default" %>
        </div>
        <div class="col-sm-2">
          <%= link_to t( 'button_label.goto_to_list' ), my_tia_lists_path, class: "btn btn-lg btn-block btn-default" %>
        </div>
      <div>
    <% end %>
  </div>
</div>
