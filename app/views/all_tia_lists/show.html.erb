<%- page_title %>
<div class="container">
  <%= breadcrumbs %>
  <%= form_title %>
  <div class="form-horizontal" role="form">
    <%= form_for @tia_list, url: all_tia_lists_path do |f| %>
      <%= render partial: 'shared/notice' %>
      <div class="form-group">
        <%= f.label :id, class: "col-sm-3 control-label" %>
        <div class="col-sm-1">
          <%= f.text_field :id, class: "form-control", readonly: true %>
        </div>
      </div>
      <div class="form-group">
        <%= f.label :code, class: "col-sm-3 control-label" %>
        <div class="col-sm-1">
          <%= f.text_field :code, class: "form-control", readonly: true %>
        </div>
      </div>
      <div class="form-group">
        <%= f.label :label, class: "col-sm-3 control-label" %>
        <div class="col-sm-3">
          <%= f.text_field :label, class: "form-control", readonly: true %>
        </div>
      </div>
      <div class="form-group">
        <%= f.label :owner_account_id, class: "col-sm-3 control-label" %>
        <div class="col-sm-1">
          <%= f.text_field :owner_account_id, class: "form-control", readonly: true %>
        </div>
      </div>
      <div class="form-group">
        <%= f.label :deputy_account_id, class: "col-sm-3 control-label" %>
        <div class="col-sm-1">
          <%= f.text_field :deputy_account_id, class: "form-control", readonly: true %>
        </div>
      </div>
      <div class="form-group">
        <%= f.label :members, class: "col-sm-3 control-label" %>
        <div class="col-sm-2">
          <%= f.text_field :members, class: "form-control", readonly: true %>
        </div>
      </div>
      <div class="form-group">
        <div class="col-sm-offset-3 col-sm-2">
          <%= link_to t( 'button_label.edit' ), edit_all_tia_list_path( @tia_list ), class: "btn btn-lg btn-block btn-default" %>
        </div>
        <div class="col-sm-2">
          <%= link_to t( 'button_label.goto_to_list' ), all_tia_lists_path, class: "btn btn-lg btn-block btn-default" %>
        </div>
      </div>
    <% end %>
  </div>
</div>
