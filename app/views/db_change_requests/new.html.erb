<%- page_title %>
<div class="container">
  <%= breadcrumbs %>
  <%= form_title %>
  <div class="form-horizontal" role="form">
    <%= form_for @dbcr do |d| %>
      <%= render partial: 'shared/errors', object: @dbcr.errors %>
      <%= render partial: 'form', object: d %>
      <div class="form-group">
        <div class="col-sm-offset-3 col-sm-2">
          <%= d.submit class: 'btn btn-lg btn-block btn-primary' %>
        </div>
        <div class="col-sm-2">
          <%= link_to t( 'button_label.cancel_to_index' ), db_change_requests_path, class: 'btn btn-lg btn-block btn-default' %>
        </div>
      </div>
    <% end %>
  </div>
</div>
