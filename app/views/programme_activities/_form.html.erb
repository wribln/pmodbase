      <div class="form-group">
        <%= form.label :id, class: 'col-sm-3 control-label' %>
        <div class="col-sm-2">
          <%= display_value form.object.to_id %>
        </div>
      </div>
      <div class="form-group">
        <%= form.label :project_id, class: 'col-sm-3 control-label' %>
        <div class="col-sm-2">
          <%= form.text_field :project_id, class: 'form-control' %>
        </div>
      </div>
      <div class="form-group">
        <%= form.label :activity_id, class: 'col-sm-3 control-label' %>
        <div class="col-sm-2">
          <%= form.text_field :activity_id, class: 'form-control' %>
        </div>
      </div>
      <div class="form-group">
        <%= form.label :activity_label, class: 'col-sm-3 control-label' %>
        <div class="col-sm-6">
          <%= form.text_field :activity_label, class: 'form-control' %>
        </div>
      </div>
      <div class="form-group">
        <%= form.label :start_date, class: 'col-sm-3 control-label' %>
        <div class="col-sm-2">
          <div class="input-group date" data-provide="datepicker">
            <%= form.text_field :start_date, class: 'form-control' %>
            <span class="input-group-addon">:::</span>
          </div>
        </div>
      </div>
      <div class="form-group">
        <%= form.label :finish_date, class: 'col-sm-3 control-label' %>
        <div class="col-sm-2">
          <div class="input-group date" data-provide="datepicker">
            <%= form.text_field :finish_date, class: 'form-control' %>
            <span class="input-group-addon">:::</span>
          </div>
        </div>
      </div>

