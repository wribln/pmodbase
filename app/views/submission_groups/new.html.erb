<%- page_title %>
<div class="container">
  <%= breadcrumbs %>
  <%= form_title %>
  <div class="form-horizontal" role="form">
    <%= form_for @submission_group do |sg| %>
      <%= render partial: 'shared/notice' %>
      <%= render partial: 'shared/errors', object: @submission_group.errors %>
      <%= render partial: 'form', object: sg %>
      <div class="form-group">
        <div class="col-sm-offset-3 col-sm-2">
          <%= sg.submit class: 'btn btn-lg btn-block btn-primary' %>
        </div>
        <div class="col-sm-2">
          <%= link_to t( 'button_label.goto_to_list' ), submission_groups_path, class: 'btn btn-lg btn-block btn-default' %>
        </div>
      </div>
    <% end %>
  </div>
</div>