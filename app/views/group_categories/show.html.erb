<%- page_title %>
<div class="container">
  <%= breadcrumbs %>
  <%= form_title %>
  <div class="form-horizontal" role="form">
    <%= form_for @group_category do |gc| %>
      <%= render partial: 'shared/notice' %>
      <div class="form-group">
        <%= gc.label :id, class: "col-sm-3 control-label" %>
        <div class="col-sm-2">
          <%= gc.text_field :id, class: "form-control", readonly: true %>
        </div>
      </div>
      <div class="form-group">
        <%= gc.label :label, class: "col-sm-3 control-label" %>
        <div class="col-sm-4">
          <%= gc.text_field :label, class: "form-control",  readonly: true %>
        </div>
      </div>
      <div class="form-group">
        <%= gc.label :seqno, class: "col-sm-3 control-label" %>
        <div class="col-sm-2">
          <%= gc.text_field :seqno, class: "form-control",  readonly: true %>
        </div>
      </div>
      <div class="form-group">
        <div class="col-sm-offset-3 col-sm-2">
          <%= link_to t( 'button_label.edit' ), edit_group_category_path( @group_category ), class: "btn btn-lg btn-block btn-default" %>
        </div>
        <div class="col-sm-2">
          <%= link_to t( 'button_label.goto_to_list' ), group_categories_path, class: "btn btn-lg btn-block btn-default" %>
        </div>
      </div>
    <% end %>
  </div>
</div>
