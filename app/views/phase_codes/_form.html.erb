      <div class="form-group">
        <%= form.label :id, class: "col-sm-3 control-label" %>
        <div class="col-sm-1">
          <%= display_value form.object.id %>
        </div>
      </div>
      <div class="form-group">
        <%= form.label :code, class: "col-sm-3 control-label" %>
        <div class="col-sm-2">
          <%= form.text_field :code, class: "form-control", max_length: MAX_LENGTH_OF_CODE, required: true %>
        </div>
      </div>
      <div class="form-group">
        <%= form.label :label, class: "col-sm-3 control-label" %>
        <div class="col-sm-5">
          <%= form.text_field :label, class: "form-control", max_length: MAX_LENGTH_OF_LABEL, required: true %>
        </div>
      </div>
      <div class="form-group">
        <%= form.label :acro, class: "col-sm-3 control-label" %>
        <div class="col-sm-2">
          <%= form.text_field :acro, class: "form-control", max_length: MAX_LENGTH_OF_CODE %>
        </div>
      </div>
      <div class="form-group">
        <%= form.label :siemens_phase_id, class: "col-sm-3 control-label" %>
        <div class="col-sm-2">
          <%= form.collection_select :siemens_phase_id, @siemens_phases, :id, :code, { prompt: true }, { class: 'form-control', required: true } %>
        </div>
      </div>
      <div class="form-group">
        <%= form.label :level, class: "col-sm-3 control-label" %>
        <div class="col-sm-1">
          <%= form.text_field :level, class: "form-control" %>
        </div>
      </div>

