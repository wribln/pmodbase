<%- page_title %>
<div class="container">
  <%= breadcrumbs %>
  <%= form_title %>
  <div class="form-horizontal" role="form">
    <%= form_for @dsr_status_record do |f| %>
      <%= render( partial: 'shared/errors', object: @dsr_status_record.errors ) %>
      <%= render( partial: 'shared/errors', object: @dsr_current_submission.errors ) unless @dsr_current_submission.nil? %>
      <%= render( partial: 'form', object: f ) %>
      <div class="form-group">
        <div class="col-sm-offset-3 col-sm-2">
          <%= f.submit class: "btn btn-lg btn-block btn-primary" %>
        </div>
        <div class="col-sm-2">
          <%= link_to t( 'button_label.cancel' ), @dsr_status_record, class: "btn btn-lg btn-block btn-default" %>
        </div>
        <div class="col-sm-2">
          <%= link_to t( 'button_label.goto_to_list' ), dsr_status_records_path, class: "btn btn-lg btn-block btn-default" %>
        </div>
      <div>
    <% end %>
  </div>
</div>
