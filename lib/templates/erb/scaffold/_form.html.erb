      <div class="form-group">
        <%%= form.label :id, class: 'col-sm-3 control-label' %>
        <div class="col-sm-2">
          <%%= display_value form.object.to_id %>
        </div>
      </div>
<% attributes.each do |attribute| -%>
      <div class="form-group">
        <%%= form.label :<%= attribute.name %>, class: 'col-sm-3 control-label' %>
        <div class="col-sm-4">
          <%%= form.text_field :<%= attribute.name %>, class: 'form-control' %>
        </div>
      </div>
<% end %>
